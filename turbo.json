{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "inputs": [
                "frontend/src/**"
            ],
            "outputs": [
                "frontend/dist/**"
            ],
            "env": [
                "VITE_APP_SERVER_URL"
            ]
        }
    }
}